<androidx.coordinatorlayout.widget.CoordinatorLayout
    android:id="@+id/root_view"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <me.zhanghai.android.fastscroll.FastScrollNestedScrollView
        android:id="@+id/scroll_view"
        style="@style/ScrollStyle">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="8dp"
            android:orientation="vertical">
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_text"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/text"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_url"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_link"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/url"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_wifi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_wifi"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/wifi"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_location"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_location"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/location"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_otp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_otp"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/otp"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_contact_vcard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_contact_white"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/contact_v_card"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_contact_mecard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_contact_white"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/contact_me_card"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_event"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_calendar"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/event"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_phone"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/phone"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_email"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/email"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_sms"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_sms"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/sms"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_mms"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_mms"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/mms"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_cryptocurrency"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_bitcoin"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/bitcoin"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_bookmark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_bookmark"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/bookmark"/>
            <com.d4rk.qrcodescanner.plus.ui.view.IconButtonWithDelimiter
                android:id="@+id/button_app"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:icon="@drawable/ic_app"
                app:iconBackground="?attr/colorPrimary"
                app:text="@string/app"
                app:isDelimiterVisible="false"/>
            <com.google.android.material.card.MaterialCardView
                style="@style/Widget.Material3.CardView.Outlined"
                android:id="@+id/card_view_ad"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/default_margin_big"
                android:layout_marginHorizontal="@dimen/default_margin"
                app:layout_constraintTop_toBottomOf="@id/card_view_about">
                <com.google.android.gms.ads.AdView
                    android:id="@+id/ad_view"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    ads:adSize="FULL_BANNER"
                    ads:adUnitId="ca-app-pub-5294151573817700/5823012328"/>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>
    </me.zhanghai.android.fastscroll.FastScrollNestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>